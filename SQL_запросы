1. SELECT C.login, COUNT(O.*)
   FROM "Couriers" as C
   INNER JOIN "Orders" as O ON C.id = O."courierId"
   WHERE (O."inDelivery") = true
   GROUP BY C.login;


2. SELECT track,
   CASE
   WHEN finished = true THEN 2
   WHEN can—Åelled = true THEN -1
   WHEN inDelivery = true THEN 1
   ELSE 0
   END AS status
   FROM "Orders";
